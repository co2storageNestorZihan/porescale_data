FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Properties of water
transportModel  Newtonian;
nu              [0 2 -1 0 0 0 0] 1e-6;  // Kinematic viscosity of water
rho             [1 -3 0 0 0 0 0] 1000;  // Density of water